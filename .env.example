#!/usr/bin/env zsh

# GitLab Server Configuration
CI_SERVER_URL=https://gitlab.com

# Registration Token (Get from GitLab: Settings > CI/CD > Runners)
# For GitLab.com: Use your project/group registration token
# For self-hosted: Use your instance registration token
REGISTRATION_TOKEN=your-registration-token-here

# Runner 1 Configuration
RUNNER_1_NAME=docker-runner-1
RUNNER_1_TAGS=docker,linux,build

# Runner 2 Configuration
RUNNER_2_NAME=docker-runner-2
RUNNER_2_TAGS=docker,linux,test

# Runner 3 Configuration
RUNNER_3_NAME=docker-runner-3
RUNNER_3_TAGS=docker,linux,deploy
